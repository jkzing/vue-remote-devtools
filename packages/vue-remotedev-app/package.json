{
  "name": "vue-remotedev-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cd dev && webpack-dev-server --inline --hot --no-info",
    "build:umd": "webpack --progress --config webpack.config.umd.js",
    "build:umd:min": "NODE_ENV=production webpack --progress --config webpack.config.umd.js",
